{
  "name": "skaut/skautis-integration",
  "description": "A WordPress plugin for integration with SkautIS",
  "type": "wordpress-plugin",
    "keywords": [
        "WordPress",
        "WordPress plugin",
        "skaut",
        "SkautIS"
    ],
  "homepage": "https://wordpress.org/plugins/skautis-integration",
  "readme": "README.md",
  "license": "proprietary",
  "authors": [
    {
      "name": "David Odehnal",
      "email": "info@davidodehnal.cz"
    },
    {
      "name": "Marek Dědič",
      "email": "developer@dedic.eu",
      "homepage": "https://dedic.eu"
    }
  ],
  "support": {
    "issues": "https://github.com/skaut/skautis-integration/issues",
    "source": "https://github.com/skaut/skautis-integration"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/skaut/Skautis.git"
    }
  ],
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "classmap-authoritative": true
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "skautis/skautis": "2.x-dev#60cf2c6f06c9d10bdc92a05e6e669518c2b8a571"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "phan/phan": "^5.1",
    "phpmd/phpmd": "^2.10",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-coding-standards/wpcs": "^3.0",
    "humbug/php-scoper": "^0.18.1",
    "skaut/wordpress-stubs": "^2.1",
    "phpstan/phpstan": "^2.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
  }
}
